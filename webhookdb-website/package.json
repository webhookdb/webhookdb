{
  "name": "webhookdb-website",
  "version": "1.0.0",
  "private": true,
  "description": "webhookdb-website",
  "author": "hello@webhookdb.com",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "scripts": {
    "develop": "gatsby develop -p 18002",
    "develop-network": "gatsby develop -H 0.0.0.0 -p 18002",
    "start": "gatsby develop",
    "copy-static": "node ./tools/copystatic.js",
    "build": "gatsby build && yarn run copy-static",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prettier": "prettier --write '*.js' './tools/*.js' './src/**/*.js' './static/**/*.js'",
    "prettier-check": "prettier --check '*.js' './tools/*.js' './src/**/*.js' './static/**/*.js'",
    "sort-imports": "import-sort --write '*.js' './tools/*.js' './src/**/*.js'",
    "sort-imports-check": "import-sort --list-different '*.js' './tools/*.js' './src/**/*.js'",
    "eslint": "eslint '*.js' './tools/*.js' './src/**/*.js' './static/**/*.js'",
    "eslint-fix": "eslint --fix '*.js' './tools/*.js' './src/**/*.js' './static/**/*.js'"
  },
  "dependencies": {
    "@deckdeckgo/highlight-code": "^3.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "bootstrap": "4.6.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.7",
    "gatsby": "^3.4.1",
    "gatsby-cli": "^3.9.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sass": "^4.6.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-remark-autolink-headers": "^4.5.0",
    "gatsby-remark-highlight-code": "^3.0.0",
    "gatsby-remark-table-of-contents": "^2.0.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.6.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "sass": "1.32.13",
    "sharp": "^0.30.4",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.31.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-webpack-plugin": "^3.0.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "prettier": "^2.3.0"
  }
}
